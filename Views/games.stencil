<html>

<body>
    <a href="/games/new">Play a new game!</a>

    <h1>Game History</h1>

    <table>
        <tr>
            <th>Player 1</th>
            <th>Player 2</th>
            <th>Result</th>
        </tr>
        {% for game in games %}
        <tr>
            <td>{{ game.p1 }}</td>
            <td>{{ game.p2 }}</td>
            <td>{{ game.result }}</td>
        </tr>
        {% endfor %}
    </table>
</body>

</html>
